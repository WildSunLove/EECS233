import static org.junit.Assert.*;

import java.util.LinkedList;

import org.junit.Test;

public class Algorithm6BTest {

	@Test
	public void test() {
		LinkedList<String> L = new LinkedList<String>();
		L.add("A");
		L.add("B");
		L.add("C");
		L.add("D");
		L.add("E");
		assertTrue(Algorithm6B.kth_largest(L, 2).equals("D"));
	}

}
